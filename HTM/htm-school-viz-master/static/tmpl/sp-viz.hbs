<div class="modal fade" id="column-history" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">SP Column Connection History</h4>
            </div>
            <div id="column-connection-history" class="modal-body">
                <div class="slider" id="column-history-slider"></div>
                <svg id="col-connections-svg"></svg>
            </div>
        </div>
    </div>
</div>

<div class="row">

    <div class="col-md-2">
        <p/>heatmap <input type="checkbox" id="heatmap">
    </div>
    <div class="col-md-2">
        <p/>histogram <input type="checkbox" id="histogram">
    </div>
    <div class="col-md-2">
        <p/>trace <input type="checkbox" id="trace">
    </div>
    <div class="col-md-2">
        <p/>synapses <input type="checkbox" id="synapses">
    </div>
    <div class="col-md-2">
        <p/>potential <input type="checkbox" id="potential">
    </div>
    <div class="col-md-2">
        <p/>connected <input type="checkbox" id="connected">
    </div>

</div>

<div class="row">

    <div class="col-md-12">
        <p/>overlap: <span id="overlap-display" class="data"></span>
    </div>

</div>

<svg id="visualization"></svg>
